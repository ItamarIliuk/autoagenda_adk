# Exemplo de arquivo .env para o AutoAgenda ADK

# Caminho para o arquivo de credenciais do Service Account
SERVICE_ACCOUNT_FILE_PATH=/caminho/para/seu-arquivo-credenciais.json

# ID da planilha do Google Sheets
GOOGLE_SHEET_ID=seu_id_da_planilha_aqui

# Configuração do Gemini API
GOOGLE_API_KEY=sua_chave_api_gemini_aqui

# Configuração para Vertex AI (FALSE para usar Google AI Studio, TRUE para Vertex AI)
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# Fuso horário (opcional, padrão é America/Sao_Paulo)
TIMEZONE=America/Sao_Paulo
